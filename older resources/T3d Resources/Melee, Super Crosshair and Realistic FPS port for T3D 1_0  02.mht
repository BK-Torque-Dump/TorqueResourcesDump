// this is the default function to call when firing a hand-to-hand =
weapon
function WeaponImage::onFireHandToHand(%this, %obj, %slot)
{
    if(%obj.hthStun) //|| %obj.shielded)
       return;
    // there was code here for special attacks
       %action =3D "Normal";
    switch$(%action)
    {
       //case "JumpAttack":
          //%attack =3D %this.jumpAttack;
       case "Normal":
          // for now we randomly choose an attack
          %index =3D mFloor(getRandom()*(%this.hthNumAttacks-0.0001));
          if (%index &gt; (%this.hthNumAttacks-1))
             %index =3D (%this.hthNumAttacks-1);
          %attack =3D %this.hthAttack[%index];
    }
    // setup the "play once look anim"
    %obj.hthDamageAttack =3D %attack;
    %obj.hthDamageSeqPlaying =3D 1;
    %obj.hthDamageStartMS =3D  $sim::Time;
    %obj.hthDamageLastId =3D -1;

    if (!%obj.ArmThreadPlayOnce(%attack.seqName))
       echo("ERROR in setArmThreadPlayOnce()");
    return;
}

// default weapon intersect
function =
WeaponImage::onImageIntersect(%this,%player,%slot,%startvec,%endvec)
{
    // if damage sequence is not playing then dont do damage
    if (!%player.hthDamageSeqPlaying || %player.getState() $=3D "Dead")
       return;

    // determine if damage is active or if we can say the seq is done =
playing
    // based on current server time
    %offset =3D $sim::Time - %player.hthDamageStartMS;

    // depending on which attack is playing...
    %attack =3D %player.hthDamageAttack;
    %startOffset =3D %attack.startDamage;
    %endOffset =3D %attack.endDamage;

    // how long until the last damage is done
    // at which point we can say the seq has "Stopped playing"
    if (%offset &gt; %endOffset)
    {
       %player.hthDamageSeqPlaying =3D 0;
       %player.hthDamageActive =3D 0;
	//   echo("seq stopping (all damage done) %offset =3D " @ %offset);
       return;
    }

    if (%offset &lt; %startOffset)
       return;

    // how long it takes for damage to start...for now we just
    // have one interval and damage is active all during that interval
    if (%offset &gt; %startOffset)
       %player.hthDamageActive =3D 1;

    // no damage yet?
    if (!%player.hthDamageActive)
    {
	//   echo("seq playing (no damage) %offset =3D " @ %offset);
       return;
    }

    // search for just players to damage
    %searchMasks =3D $TypeMasks::PlayerObjectType | =
$TypeMasks::StaticShapeObjectType;
    // search for objects within the damage rays that fit the masks =
above
    %scanTarg =3D ContainerRayCast(%startvec, %endvec, %searchMasks, =
%slot);

    if(%scanTarg &amp;&amp; (%scanTarg.getType() &amp; =
$TypeMasks::PlayerObjectType))
    {      =20
        // a target in range was found
        %target =3D firstWord(%scanTarg);

        // store end point from raycast return buffer
        %pos =3D getWords(%scanTarg, 1, 3);

        // if we have hit this person already...apply no more damage
        if (%target =3D=3D %player.hthDamageLastId)
           return;

        // save who we last damaged
        %player.hthDamageLastId =3D %target;

        // Apply damage targetted object
   		// Works for all shapebase objects.
        if (%target.getState() !$=3D "Dead" &amp;&amp; %target.getId() =
!$=3D %player.getId())
   		{
   		   // shields and sword rebounding - JM The_Force
           if(%target.shielded || %target.hthDamageSeqPlaying)
           {
              if(%target.hthDamageSeqPlaying)
                 %swordBlock =3D 1;
              %block =3D 1;
              // Now we see if we hit from behind...
              %forwardVec =3D %target.getEyeVector();
              %objDir2D   =3D getWord(%forwardVec, 0) @ " " @ =
getWord(%forwardVec,1) @ " " @ "0";
              %objPos     =3D %target.getPosition();
              %dif        =3D VectorSub(%objPos, %player.getPosition());
              %dif        =3D getWord(%dif, 0) @ " " @ getWord(%dif, 1) =
@ " 0";
              %dif        =3D VectorNormalize(%dif);
              %dot        =3D VectorDot(%dif, %objDir2D);
              // 120 Deg angle test...
              // 1.05 =3D=3D 60 degrees in radians
              if (%dot &gt;=3D mCos(1.05))
                 %block =3D 0;
           }
           if(%block =3D=3D 1)
           {
              //error("attack blocked!!");
              //%player.playAudio(0,ShieldImpactSound);
              //makeSparks(%pos);
              stunPlayer(%player,%attack);
              pushPlayerBack(%player,%pos,%target,%attack);
              if(%swordBlock =3D=3D 1)
              {
                 stunPlayer(%target,%attack);
                 pushPlayerBack(%target,%pos,%player,%attack);
              }
              return;
           }
           //AISK Changes: Start

           //If friendly fire is turned off, and the source and target =
are on
           //the same team, then return
           if ($AISK_FRIENDLY_FIRE =3D=3D false &amp;&amp; =
$AISK_FREE_FOR_ALL =3D=3D false)
           {
                if (%target.team =3D=3D %player.team)
                    return;
           }

           if (%target.isbot =3D=3D true)
           {
              %target.attentionlevel =3D 1;
              %target.enhancefov(%obj);

            if (%target.behavior $=3D "npc")
                return;
           }

           %damage =3D %attack.damageAmount;
           %damageType =3D %this.item; // example: Axe / Sword etc

           %damLoc =3D firstWord(%target.getDamageLocation(%pos));
           // you can use this to add limited loacational damage, but =
the head is whats hit the most - TF
           //if(%damLoc $=3D "head")
              //error("object sliced on head");
           //else if(%damLoc $=3D "torso")
              //error("object sliced on torso");
           //else if(%damLoc $=3D "legs")
              //error("object sliced on legs");

           // code ripped from Armor::damgae
  		   %target.applyDamage(%damage);

           // this is in the Armor::damage
           //%location =3D "Body";

           // Deal with client callbacks here because we don't have this
           // information in the onDamage or onDisable methods
           %client =3D %target.client;
           %sourceObject =3D %this;
           %sourceClient =3D %sourceObject ? %sourceObject.client : 0;

           if (%target.getState() $=3D "Dead")
           {
              //Respawn the bot if needed
              if (%target.isbot =3D=3D true)
              {
                 if (%target.respawn =3D=3D true)
                 {
                    %target.marker.delayRespawn =3D =
schedule($AISK_RESPAWN_DELAY, %target.marker, "AIPlayer::spawn", =
%target.botname, %target.marker);
                    %this.player =3D 0;
                 }
              }
              else
                 %client.onDeath(%sourceObject, %sourceClient, =
%damageType, %pos);
           }
           //AISK Changes: End
           else
           {
              // stun and pushBack can be different for each attack
              if (%attack.stunedTime &gt; 0)
                  stunPlayer(%target,%attack);
              if (%attack.pushBack &gt; 0)
                  pushPlayerBack(%target,%pos,%player,%attack);
           }
      	}
    }
    else if(%scanTarg &amp;&amp; (%scanTarg.getType() &amp; =
$TypeMasks::StaticShapeObjectType))
    {
       %damage =3D %attack.damageAmount;
       %object =3D firstWord(%scanTarg);
       %object.applyDamage(%damage);
    }
}

// call when %victim is hit with %attack but does not die
function stunPlayer(%vplayer, %attack)
{
   // for now we stun every time...

   // get the player for this object
   //if (!%victim.client || !%victim.client.player)
   if(!%vplayer.getType() &amp; $TypeMasks::PlayerObjectType)
   {
      error("ATTEMPTING to STUN a non-player");
      return;
   }

   // if this player is in the middle of a hth swing themself, then
   // their swing is aborted. firstly we have to make sure they dont
   // do any damage, secondly we must blend their swing anim into
   // the stun anim
   if (%vplayer.hthDamageSeqPlaying)
   {
      // make sure they wont do damage....
      %vplayer.hthDamageSeqPlaying =3D false;

      // blend into the stun animation
      //error("STUN: victim: " @ %vplayer @ " DOING transition =
once...");
      %vplayer.ArmThreadTransitionOnce("h1stun");
   }
   else
   {
      // just start the stun animation
      //error("STUN: victim: " @ %vplayer @ " only doing a =
playonce...");
      if(%vplayer.shielded)// not while stuned!
         %vplayer.setImageTrigger(1,false);
      %vplayer.ArmThreadPlayOnce("h1stun");
   }

   // the victim is now in a stun state
   //%vplayer.hthStunSequencePlaying =3D true;
   %vplayer.hthStun =3D true;
   schedule(%attack.stunedTime, %vplayer, "resetStun", %vplayer);
   //%vplayer.hthStunStartMS =3D $sim::Time;
}

function resetStun(%obj)
{
   %obj.hthStun =3D false;
}

function pushPlayerBack(%victim, %pos, %attacker, %attack)
{
  // the push back is relative to the attacker
  // a straight push back would be along the attackers
  // Y axis....

  // right now we always push the victim at his center
  // we could explore what happnes if we push at the
  // point of contact instead (might turn or do something intersting)

  // get the usual direction to push...we could get the Y axis of
  // the attacker with getTransform() then grabbing the rotation part
  // and passing that to VectorOrthoBasis() and then using column 1
  // whichi would be words 3,4,5 (couting from 0)...but that's overkill
  // for something that can be approximated pretty good by a line drawn
  // from attacker to victim...so let's use that instead
  %vpos =3D %victim.getWorldBoxCenter();
  %pushDirection =3D VectorSub(%vpos,%attacker.getWorldBoxCenter());
  %pushDirection =3D VectorNormalize(%pushDirection);

  // per attack impluse
  %impulse =3D %attack.pushBack;

  // ok apply impulse to victim's center
  %mass =3D %victim.getDataBlock().mass;
  %pushVec =3D VectorScale(%pushDirection,%impulse * %mass);

  //error("Applying, to player " @ %victim @ " of mass " @ %mass @ ", an =
impulseVec: " @ %pushVec);

  %victim.applyImpulse(%vpos, %pushVec);
}
