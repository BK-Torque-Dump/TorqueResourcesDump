damage =3D mClampF(1 - damage, 0, 1);

// Center the bar
RectI rect(offset, mDamageRectSize);
rect.point.x -=3D mDamageRectSize.x / 2;

// Draw the border
//dglDrawRect(rect, mDamageFrameColor);
 GFX-&gt;getDrawUtil()-&gt;drawRect(rect, mDamageFrameColor);

// Draw the damage % fill
rect.point +=3D Point2I(1, 1);
rect.extent -=3D Point2I(1, 1);
rect.extent.x =3D (S32)(rect.extent.x * damage);
if (rect.extent.x =3D=3D 1)
rect.extent.x =3D 2;
if (rect.extent.x &gt; 0)
//dglDrawRectFill(rect, mActiveColor);
GFX-&gt;getDrawUtil()-&gt;drawRectFill(rect, mActiveColor);
}

