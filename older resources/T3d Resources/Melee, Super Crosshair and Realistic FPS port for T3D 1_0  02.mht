//-----------------------------------------------------------------------=
-------
Point2I GuiCrossHairHud::avgSmoothArray()
{
U8 i;
Point2I average(0,0);

for (i=3D0;i&lt;SMOOTH_FACTOR;i++)
{
average.x +=3D mSmooth[i].x;
average.y +=3D mSmooth[i].y;
}
average.x /=3D SMOOTH_FACTOR;
average.y /=3D SMOOTH_FACTOR;
return average;

}

