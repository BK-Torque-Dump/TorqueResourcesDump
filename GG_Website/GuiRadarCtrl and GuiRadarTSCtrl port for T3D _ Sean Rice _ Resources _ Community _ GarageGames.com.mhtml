   F32 getRange() { return mRange; }
   void setEllipticColor(ColorF color) { mEllipticColor =3D color; }
   void setSphereColor(ColorF color) { mSphereColor =3D color; }
   void setBlipColor(ColorF color) { mBlipColor =3D color; }
   void setStemColor(ColorF color) { mStemColor =3D color; }

	void setTarget(const char *TargetShapeBaseName);

   DECLARE_CONOBJECT(GuiRadarTSCtrl);
};

