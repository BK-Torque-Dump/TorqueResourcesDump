// set up the camera position for rendering the 3D scene within this contro=
l
bool GuiRadarTSCtrl::processCameraQuery(CameraQuery *camq)
{
   // pretty hacky hardcoded values
   // gives a reasonable viewing angle
   camq-&gt;nearPlane =3D 0.1f; // near clip plane
   camq-&gt;farPlane =3D 20.0f; // far clip plane
   camq-&gt;fov =3D (F32)M_PI/6;     // field of view
   MatrixF cam;
   cam.set(EulerF((F32)M_PI/8, 0, 0)); // rotation
   cam.setColumn(3, Point3F(0, -4, 1.65f)); // position
   camq-&gt;cameraMatrix =3D cam;
   return (true);
}

