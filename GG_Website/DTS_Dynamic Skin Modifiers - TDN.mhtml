   for( U32 i =3D 0; i &lt; 4; i++ )
   {
      ext =3D dStrstr(pName, extArray[i]);
      if (ext&nbsp;!=3D NULL)
      {
         U32 len =3D dStrlen(pName);
         dStrcpy(newM, pName);
         newM[len-4] =3D '\0';
         break;
      }
   }
   if (ext =3D=3D NULL)
   {
      U32 len =3D dStrlen(pName);
      dStrcpy(newM, pName);
      newM[len] =3D '\0';
   }

   // Make a texture file pathname with the new root if this name
   // has the old root in it; otherwise just make a path with the
   // original name.
   makeSkinPath(skinPath, NAME_BUFFER_LENGTH, resourcePath,
                        pName, defaultBaseName, skinName.getString());
   // If no mask specified we are only changing the skin and getting outta =
here without anything else
   if (!maskName.isValidString())
   {
      removeSkinModifier(slot);
      mSkinModifiers2[slot].matIndex =3D index;
      mSkinModifiers2[slot].skinName =3D skinName;
      mSkinModifiers2[slot].skinPath =3D skinPath;
      dSprintf(mSkinModifiers2[slot].modTag,32,"+[%d+0]",_StringTable::hash=
String(skinName.getString()));
      return;
   }
   makeSkinPath(maskPath, NAME_BUFFER_LENGTH, resourcePath,
                        newM, defaultBaseName, maskName.getString());
   GBitmap *maskBmp=3DTextureManager::loadBitmapInstance(maskPath);
   // fail if no mask found
   if (!maskBmp)
   {
      Con::errorf("Can't open mask! %s %s", newM, maskName.getString());
      return;
   }
   // This one if our bitmap where most "magic" happens
   GBitmap *printBmp=3Dnew GBitmap(maskBmp-&gt;getWidth(),maskBmp-&gt;getHe=
ight(),false,GBitmap::RGBA);
   // Make it "black" transparent first
   printBmp-&gt;fill(ColorI(0,0,0,0));
   // Fill &amp; Blend if needed
   if (cnt=3D=3D1)
      printBmp-&gt;fillByChannels(*maskBmp, chn1, clr1);
   else if (cnt=3D=3D2)
      printBmp-&gt;fillByChannels(*maskBmp, chn1, chn2, clr1, clr2);
   else if (cnt=3D=3D3)
      printBmp-&gt;fillByChannels(*maskBmp, chn1, chn2, chn3, clr1, clr2, c=
lr3);
   else if (cnt=3D=3D4)
      printBmp-&gt;fillByChannels(*maskBmp, chn1, chn2, chn3, chn4, clr1, c=
lr2, clr3, clr4);
   else
      Con::errorf("No colors assigned! Will use transparent picture on fill=
ing! (why calling with mask and no colors?)");
   // creating modifier
   removeSkinModifier(slot);
   mSkinModifiers2[slot].matIndex =3D index;
   mSkinModifiers2[slot].skinName =3D skinName;
   mSkinModifiers2[slot].skinPath =3D skinPath;
   mSkinModifiers2[slot].modBmp=3DprintBmp;
   dSprintf(mSkinModifiers2[slot].modTag,32,"+[%d+%d+%d+%d+%d+%d]",_StringT=
able::hashString(skinName.getString()),
                                                          _StringTable::has=
hString(maskName.getString()),=20
                                                          clr1.alpha, clr2.=
alpha, clr3.alpha, clr4.alpha);
   if (maskBmp)
      delete maskBmp;
}
</pre>
