<h4> void TSShapeInstance::modifySkins() </h4>
<pre>void TSShapeInstance::modifySkins()
{
   char skinName[512];

   // Make our own copy of the materials list from the resource
   // if necessary.
   if (!ownMaterialList()) {
      cloneMaterialList();
   }

   TSMaterialList* pMatList =3D getMaterialList();
   for (U32 mm=3D0; mm&lt;pMatList-&gt;mMaterialNames.size(); mm++)
   {
      // Get the name of this material.
      const char* baseSkinName=3DpMatList-&gt;mMaterialNames[mm];
      // a different skin could be in use, so use that name instead
      const char* materialName=3DpMatList-&gt;mMaterials[mm].getName();
      if(materialName =3D=3D NULL)
      {
          Con::errorf("DEBUG: ModifySkins: No existing skin in use!");
          continue;
      }
      dStrcpy(skinName,materialName);
      char *modTag=3DdStrstr((const char*)skinName,"+[");
      if (modTag)
      {
         skinName[modTag-skinName]=3D'\0';
      }
      // modify skin
      pMatList-&gt;mMaterials[mm]=3DmodifySkin(baseSkinName,skinName, mm);
   }
}
</pre>
<div class=3D"editsection" style=3D"float:right;margin-left:5px;">[<a href=
=3D"http://tdn.garagegames.com/wiki/index.php?title=3DDTS/Dynamic_Skin_Modi=
fiers&amp;action=3Dedit&amp;section=3D34" title=3D"DTS/Dynamic Skin Modifie=
rs">edit</a>]</div><a name=3D"long_TSShapeInstance::whichMaterial.28const_c=
har_.2AskinLoc.29"></a><h4> long TSShapeInstance::whichMaterial(const char =
*skinLoc) </h4>
<pre>long TSShapeInstance::whichMaterial(const char *skinLoc)
{
   //Con::errorf("TSShapeInstance::whichMaterial(%s)",skinLoc);
   char *period, *specificName;

   // Make our own copy of the materials list from the resource if necessar=
y.
   setMaterialList(mShape-&gt;materialList);
   if (ownMaterialList() =3D=3D false)
      cloneMaterialList();

   // This one is also a bit hacky. Please see if it's possible to improve

   //Con::errorf("whichMaterial(%s)",skinLoc);
   // Cycle through the materials.
   TSMaterialList* pMatList =3D getMaterialList();
   for (S32 j =3D 0; j &lt; pMatList-&gt;mMaterialNames.size(); j++)
   {
      // Get the name of this material.
      const char* pName =3D pMatList-&gt;mMaterialNames[j];
      //Con::warnf("index=3D%d, name=3D%s",j,pName);
      // Bail if no name.
      if (pName =3D=3D NULL)
      continue;

      // find the second part of the material name
      period =3D dStrchr((char *) pName, '.');
      if (period =3D=3D NULL)
         continue;
      char *begin =3D dStrchr((char *) pName, '.') + 1;
      char *end =3D begin;
      char *t;
      while((t =3D dStrchr(end, '.')))
         end =3D t + 1;
      const char* extArray[4] =3D { "jpg", "png", "gif", "bmp" };
      for( U32 i =3D 0; i &lt; 4; i++ )
      {
         if (dStricmp(end, extArray[i]) =3D=3D 0)
         {
            char buf[128];
            S32 i;
            for(i =3D 0; begin &lt; end - 1; ++i, ++begin)
            buf[i] =3D *begin;
            if (buf =3D=3D NULL)
               continue;
            buf[i] =3D '\0';
            begin =3D buf;
            if (dStricmp(buf, skinLoc) =3D=3D 0)
               return(j);
            break;
         }
      }
      if (dStricmp(end, skinLoc)=3D=3D 0)
         return(j);
   }
   return(-1);
}
</pre>
<div class=3D"editsection" style=3D"float:right;margin-left:5px;">[<a href=
=3D"http://tdn.garagegames.com/wiki/index.php?title=3DDTS/Dynamic_Skin_Modi=
