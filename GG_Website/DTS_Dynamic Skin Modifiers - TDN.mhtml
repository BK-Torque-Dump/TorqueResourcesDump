void GBitmap::fill(const U8 in_r, const U8 in_g, const U8 in_b, const U8 in=
_a)
{
   fill(ColorI(in_r, in_g, in_b, in_a));
}

void GBitmap::fill(ColorI fColor)
{
   for (U32 yy=3D0; (yy&lt;height); yy++)
   {
      for (U32 xx=3D0; (xx&lt;width); xx++)
      {
         setColor(xx,yy,fColor);
      }
   }
}

// one channel case
void GBitmap::fillByChannels(const GBitmap&amp; rBlend, U8 channel, ColorI =
fColor)
{
   for (U32 yy=3D0; (yy&lt;height) &amp;&amp; (yy&lt;rBlend.height); yy++)
   {
      for (U32 xx=3D0; (xx&lt;width) &amp;&amp; (xx&lt;rBlend.width); xx++)
      {
         ColorI tempColor;
         rBlend.getColor(xx,yy,tempColor);
         U8 level;
         if (channel=3D=3D0) level =3D tempColor.red;
         else if (channel=3D=3D1) level =3D tempColor.green;
         else if (channel=3D=3D2) level =3D tempColor.blue;
         else level =3D tempColor.alpha;

         if (level&nbsp;!=3D 0 )
         {
            ColorI fill =3D ColorI(0,0,0,0);
            F32 t;
            t =3D fColor.red * level /255;
            fill.red =3D U8(t);
            t =3D fColor.green * level /255;
            fill.green =3D U8(t);
            t =3D fColor.blue * level /255;
            fill.blue =3D U8(t);
            t =3D fColor.alpha * level /255;
            fill.alpha =3D U8(t);
            setColor(xx, yy, fill);
         }

      }
   }
}

// two channels case
void GBitmap::fillByChannels(const GBitmap&amp; rBlend,=20
   U8 channel1, U8 channel2, ColorI fColor1, ColorI fColor2)
{
   for (U32 yy=3D0; (yy&lt;height) &amp;&amp; (yy&lt;rBlend.height); yy++)
   {
