#include "platform/platform.h"

#include "gui/core/guiControl.h"
#include "gfx/gfxDrawUtil.h"
#include "T3D/player.h"

#include "console/console.h"
#include "console/consoleTypes.h"
#include "gfx/gfxDevice.h"
#include "app/game.h"
#include "T3D/gameConnection.h"
#include "T3D/ShapeBase.h"
#include "gfx/primBuilder.h"

#include "gui/controls/GuiRadarCtrl.h"

IMPLEMENT_CONOBJECT(GuiRadarCtrl);

GuiRadarCtrl::GuiRadarCtrl(void)
{
	// Radar Background default
	mRadarBitmapName =3D =
StringTable-&gt;insert("art/gui/compass_map_radar/radar_base.png");

	// Blip defaults
	mBlipUpName =3D =
StringTable-&gt;insert("art/gui/compass_map_radar/dot_teal.png");
	mBlipBelowName =3D =
StringTable-&gt;insert("art/gui/compass_map_radar/dot_black.png");
	mBlipLevelName =3D =
StringTable-&gt;insert("art/gui/compass_map_radar/dot_red.png");
	mShowRadar =3D true;
	//mRadarWrap =3D false;
	mRadarRadiusRange =3D 100.0f;

	mLevelRange =3D 20.0f;

	// As default show only players
	mShowShapeBase =3D false;
	mShowVehicles =3D false;
	mShowPlayers =3D true;
    mShowBots =3D true;

	// Compass defaults
	mCompassBitmapName =3D =
StringTable-&gt;insert("art/gui/compass_map_radar/compass.png");
	mShowCompass =3D true;

	mCompassRotation =3D 0.0f;

	// &lt;MH&gt;
	// Map defaults
	mMapBitmapName =3D =
StringTable-&gt;insert("art/gui/compass_map_radar/scorchedPlanet.png");
	mShowMap =3D true;
	// &lt;/MH&gt;

	mHideAll =3D false;
}


void GuiRadarCtrl::initPersistFields()
{
   Parent::initPersistFields();

    addGroup("Radar");
    addField("RadarBitmap",		TypeFilename,	  Offset(mRadarBitmapName,    =
GuiRadarCtrl));
    //addField("RadarWrap",		TypeBool,		  Offset(mRadarWrap,          =
GuiRadarCtrl));
    addField("blip_above",		TypeFilename,     Offset(mBlipUpName,        =
 GuiRadarCtrl));
	addField("blip_below",		TypeFilename,     Offset(mBlipBelowName,      =
GuiRadarCtrl));
	addField("blip_level",		TypeFilename,     Offset(mBlipLevelName,      =
GuiRadarCtrl));
	addField("RadarRange",		TypeF32,		  Offset(mRadarRadiusRange,   =
GuiRadarCtrl));
	addField("ShowRadar",		TypeBool,		  Offset(mShowRadar,          =
GuiRadarCtrl));
	endGroup("Radar");
  =20
	addGroup("Compass");
	addField("CompassBitmap",	TypeFilename,	Offset(mCompassBitmapName, =
GuiRadarCtrl));
	addField("ShowCompass",		TypeBool,		Offset(mShowCompass,       =
GuiRadarCtrl),"Show Compass)");
    endGroup("Compass");	=09

	// &lt;MH&gt;
	addGroup("Map");
	addField("MapBitmap",		TypeFilename,	Offset(mMapBitmapName,    =
GuiRadarCtrl));
	addField("ShowMap",			TypeBool,		Offset(mShowMap,		  GuiRadarCtrl), =
"Show Map");
    endGroup("Map");=09
	// &lt;/MH&gt;
=09
	addGroup("Misc");
	addField("RadarShowPlayers",	TypeBool,     Offset(mShowPlayers,     =
GuiRadarCtrl),"Show Players on Radar");
	addField("RadarShowVehicles",	TypeBool,     Offset(mShowVehicles,    =
GuiRadarCtrl),"Show Vehicles on Radar");
    addField("RadarShowBots",		TypeBool,     Offset(mShowBots,        =
GuiRadarCtrl),"Show Bots on Radar");
	addField("RadarShowShapeBase",	TypeBool,     Offset(mShowShapeBase,   =
GuiRadarCtrl),"Show All ShapeBase derived objects on Radar");
	addField("LevelRange",			TypeS32,      Offset(mLevelRange,      =
GuiRadarCtrl),"The height difference at which the Level blip is =
displayed");
	addField("HideAll",				TypeBool,     Offset(mHideAll,         =
GuiRadarCtrl));
	addField("ShowFrame",			TypeBool,     Offset(mShowFrame,       =
GuiRadarCtrl));
	endGroup("Misc");
}

ConsoleMethod( GuiRadarCtrl, setValue, void, 4, 4, "(int xAxis, int =
yAxis)"
              "Set the offset of the bitmap.")
{
	object-&gt;setValue(dAtoi(argv[2]), dAtoi(argv[3]));
}

ConsoleMethod( GuiRadarCtrl, setRadarBitmap, void, 3, 4, "(string =
filename, bool resize=3Dfalse)"
               "Set the bitmap displayed in the control. Note that it is =
limited in size, to 256x256.")
{
   char fileName[1024];
   Con::expandScriptFilename(fileName, sizeof(fileName), argv[2]);
   object-&gt;setRadarBitmap(fileName, argc &gt; 3 ? dAtob( argv[3] ) : =
false );
}

ConsoleMethod( GuiRadarCtrl, setRadarRange, void, 3, 3, "")
{
	Con::printf("Object-&gt;SetRadius =3D %f", dAtof(argv[2]));
