Sphere::TriangleMesh *sphereMesh =3D sphere.getMesh(mSphereDetail); // =
sphere.getMesh(2) gives a slighty 'chunky' sphere - go higher if you can =
handle the framerate hit
	S32 numPoly =3D sphereMesh-&gt;numPoly;
	S32 totalPoly =3D 0;

	GFXVertexBufferHandle&lt;GFXVertexPC&gt; verts(GFX, numPoly*3, =
GFXBufferTypeVolatile);
	verts.lock();
	S32 vertexIndex =3D 0;
	for (S32 i=3D0; i&lt;numPoly; i++)
	{
		totalPoly++;

		verts[vertexIndex].point =3D sphereMesh-&gt;poly[i].pnt[0];
		verts[vertexIndex].color =3D mSphereColor;
		vertexIndex++;

		verts[vertexIndex].point =3D sphereMesh-&gt;poly[i].pnt[1];
		verts[vertexIndex].color =3D mSphereColor;
		vertexIndex++;

		verts[vertexIndex].point =3D sphereMesh-&gt;poly[i].pnt[2];
		verts[vertexIndex].color =3D mSphereColor;
		vertexIndex++;
	}
	verts.unlock();

	GFXStateBlockDesc desc2;
	desc2.setCullMode(GFXCullNone);
	desc2.zEnable =3D false;
	desc2.setBlend(true, GFXBlendSrcAlpha, GFXBlendInvSrcAlpha);

	GFX-&gt;setStateBlockByDesc( desc2 );
	GFX-&gt;setupGenericShaders( GFXDevice::GSColor );

	GFX-&gt;setVertexBuffer( verts );
	GFX-&gt;drawPrimitive( GFXTriangleList, 0, totalPoly );

	GFX-&gt;setStateBlockByDesc( desc );
	GFX-&gt;setupGenericShaders();</PRE></DIV>
