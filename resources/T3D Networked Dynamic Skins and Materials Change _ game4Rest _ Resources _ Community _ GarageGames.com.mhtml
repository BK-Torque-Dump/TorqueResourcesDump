//Handles keyboard input to change materials
$CurrentFacePartToApplyChange =3D 0;  //0: Base texture, 1: Monocle, 2:Chee=
k, 3: beard
$FaceTexNo[0] =3D 0;  //Current Base Texture Number
$FaceTexNo[1] =3D 0;  //Mark 1
$FaceTexNo[2] =3D 0;  //Mark 2
$FaceTexNo[3] =3D 0;  //Mark 3

function changeFaceMaterial(%value)
{
   //Instead of global variables, I check locally if requested
   //texture number is out limitation. Server will check it out again.
   //But client needs to do this to minimize the amount of meaningless data=
.=20
   //Texture numbers. Base:4, Mark1:4, Mark2:5, Mark3:3
   %selectedFacePart =3D $CurrentFacePartToApplyChange;
   %selecteFaceTexNo =3D 0;
   if(%selectedFacePart =3D=3D 0)  //Base TExture
   {
      %texNo =3D $FaceTexNo[0] + %value;
      if(%texNo &gt;=3D0 &amp;&amp; %texNo &lt;=3D 3)
      {
         $FaceTexNo[0] =3D %texNo;
         %selecteFaceTexNo =3D %texNo;
      }
      else
         return;
   }
   if(%selectedFacePart =3D=3D 1)  //Mark1
   {
       %texNo =3D $FaceTexNo[1] + %value;
      if(%texNo &gt;=3D0 &amp;&amp; %texNo &lt;=3D 3)
      {
         $FaceTexNo[1] =3D %texNo;
         %selecteFaceTexNo =3D %texNo;
      }
      else
         return;
   }
   if(%selectedFacePart =3D=3D 2)  //Mark2
   {
       %texNo =3D $FaceTexNo[2] + %value;
      if(%texNo &gt;=3D0 &amp;&amp; %texNo &lt;=3D 4)
      {
         $FaceTexNo[2] =3D %texNo;
         %selecteFaceTexNo =3D %texNo;
      }
      else
         return;
   }
    if(%selectedFacePart =3D=3D 3)  //Mark3
   {
       %texNo =3D $FaceTexNo[3] + %value;
      if(%texNo &gt;=3D0 &amp;&amp; %texNo &lt;=3D 2)
      {
         $FaceTexNo[3] =3D %texNo;
         %selecteFaceTexNo =3D %texNo;
      }
      else
         return;
   }
  =20
   updateFaceMaterial(%selectedFacePart,%selecteFaceTexNo);
}

function selectToChange(%value)
{
   %selectedPart =3D $CurrentFacePartToApplyChange + %value;
   if(%selectedPart &gt; 3 || %selectedPart &lt; 0)
      return;
  =20
   $CurrentFacePartToApplyChange =3D %selectedPart;
}


//
//** CHANGING FACE MATERIAL functions--------------------------------------=
------//
//
function updateFaceMaterial(%targetIdx, %value)
{ =20
   if(%targetIdx =3D=3D 0)
      updateFaceBase(%targetIdx,%value);
   else if(%targetIdx =3D=3D 1)
      updateFaceMark1(%targetIdx,%value);
   else if(%targetIdx =3D=3D 2)
      updateFaceMark2(%targetIdx,%value); =20
   else if(%targetIdx =3D=3D 3)
      updateFaceMark3(%targetIdx,%value);
}

function updateFaceBase(%targetIdx,%value)
{
   commandToServer('ReqUpdateFaceMaterial', %targetIdx, %value);=20
}

function updateFaceMark1(%targetIdx,%value)
{
   commandToServer('ReqUpdateFaceMaterial', %targetIdx, %value);
}

function updateFaceMark2(%targetIdx,%value)
{
  commandToServer('ReqUpdateFaceMaterial', %targetIdx, %value);
}

function updateFaceMark3(%targetIdx,%value)
{
   commandToServer('ReqUpdateFaceMaterial', %targetIdx, %value);
}

