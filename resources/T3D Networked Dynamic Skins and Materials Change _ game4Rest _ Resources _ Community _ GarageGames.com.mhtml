//
//** Script Object to save face material related information.
//FACE
$Face     =3D new ScriptObject(FaceObj)
                  {
                     meshObjectArray   =3D "";
                    =20
                     //Defines base textures in the array. It will be used =
for diffuseMap[0]
                     baseTexArray =3D "head_d f500 f501 f502";=20
                    =20
                     //Each base texture can have 3 types of marks.
                     //The following is the marks for the first base textur=
e from teh baseTexArray      =20
                     FirstMarkArray0  =3D "c0 f200 f201 f202";        //Mon=
ocle
                     SecondMarkArray0 =3D "c0 f100 f101 f102 f103";	 //Chee=
ks=09
                     ThirdMarkArray0  =3D "c0 f300 f301";    //Beard
                    =20
                     //We can define another marks like this.
                     //We can define another marks for the second base text=
ure like this.
                     //FirstMarkArray1  =3D "c0 f250 f251 f252";
                     //SecondMarkArray1 =3D "c0 f150 f151 f152 f153";
                     //ThirdMarkArray1  =3D "c0 f350 f351";
                    =20
                     //If we need to define commonly used marks for all bas=
e textures, we can do like this.
                     //FirstCommonMarkArray  =3D "c0 f11";
                     //SecondCommonMarkArray =3D  "c0 f12";
                     //ThirdCommonMarkArray  =3D "c0 f21 f22";             =
       =20
                  };


//
//Got request from client to change face material
//
function serverCmdReqUpdateFaceMaterial(%client, %targetIdx, %value)
{
   %client.updateFaceSkins(%targetIdx, %value);
}


function GameConnection::initDynamicFaceMaterial(%this)
{
   %this.FaceMat =3D "F head_d 0 0 0 0";
  =20
   %this.FaceTex[0] =3D "head_d";
   %this.FaceTex[1] =3D "c0";
   %this.FaceTex[2] =3D "c0";
   %this.FaceTex[3] =3D "c0"; =20
}

//
//** Face skins and marks change-------------------------- ----------------=
-----------//
//
function GameConnection::updateFaceSkins(%this, %targetIdx, %value)
{
   %currentMat  =3D %this.FaceMat;
   %texName =3D getFaceTextureName(%targetIdx, %value);  //Get name from ar=
ray
   %count   =3D getWordCount(%currentMat)-1;
  =20
   //Get new material Name
   if(%targetIdx &lt; 5)
      %newMatName =3D getWords(%currentMat, 0, %targetIdx+1) @ " " @ %value=
 @ " "   //SPC gives me compile error???
                             @ getWords(%currentMat, %targetIdx+3, %count);
   else if(%targetIdx =3D=3D 5)
      %newMatName =3D getWords(%currentMat, 0, %targetIdx+1) @ " " @ %value=
;
   else
      return;
  =20
   //Just disregard if new material name is same to the current.
   if(%currentMat $=3D %newMatName)
      return;
  =20
  =20
   %player =3D %this.player;
   %mapTo =3D "head_d";
  =20
   %this.FaceTex[%targetIdx] =3D %texName;
   %player.changeShapeSkin(%newMatName, %mapTo, %this.FaceTex[0],%this.Face=
Tex[1],
                           %this.FaceTex[2],%this.FaceTex[3]);
  =20
   //Save the change
   %this.faceMat =3D %newMatName;=20
}


function getFaceTextureName(%targetIdx, %value)
{
   if(%targetIdx =3D=3D 0)     //Base Texture
      %texName =3D getWord(FaceObj.baseTexArray, %value);
   else if(%targetIdx =3D=3D 1) //Mark1
      %texName =3D getWord(FaceObj.FirstMarkArray0, %value);
   else if(%targetIdx =3D=3D 2) //Mark2
      %texName =3D getWord(FaceObj.SecondMarkArray0, %value);
   else if(%targetIdx =3D=3D 3) //Mark3
      %texName =3D getWord(FaceObj.ThirdMarkArray0, %value);
  =20
   return %texName;
