   debrisVelocityVariance =3D 0.2;//0.5;

   lightStartRadius =3D 6.0;
   lightEndRadius =3D 0.0;
   lightStartColor =3D "1.0 0.7 0.2";
   lightEndColor =3D "0.9 0.7 0.0";
