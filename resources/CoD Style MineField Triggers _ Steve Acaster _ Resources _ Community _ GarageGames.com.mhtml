{
   // The period is value is used to control how often the console
   // onTriggerTick callback is called while there are any objects
   // in the trigger.  The default value is 100 MS.
   tickPeriodMS =3D 1000;
};

function MineFieldTrigger::onTickTrigger(%this,%trigger)
{
   // This method is called every tickPerioMS, as long as any
   // objects intersect the trigger.

   // You can iterate through the objects in the list by using these
   // methods:
   //    %trigger.getNumObjects();
   //    %trigger.getObject(n);
  =20
    %poor_fools =3D %trigger.getNumObjects();
	for(%x=3D0; %x &lt; %poor_fools; %x++)
	{
		%mr_unlucky =3D %trigger.getObject(%x);
	=09
		//check it's a player! Also check it's not already dead - shouldn't be re=
-exploding on corpses
		//No point trying to blow up something we can't blow up
		//there are other things with trigger callbacks which may not be damageab=
le
		//eg: items=20
		//player classname is good for Ai too
		if(%mr_unlucky.getClassName() $=3D"Player" &amp;&amp; %mr_unlucky.getStat=
e() !$=3D"Dead")
		{
		    //and spawn the stock grenadelauncher explosion
			%blast =3D new explosion()
			{
				dataBlock =3D "RocketLauncherExplosion";
				position =3D %mr_unlucky.getPosition();
			};
			MissionCleanup.add(%blast);
		=09
			//and kill the player outright
			%mr_unlucky.kill();
		}
	}
